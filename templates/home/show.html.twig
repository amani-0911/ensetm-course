{% extends 'base.html.twig' %}

{% block title %}Filieres{% endblock %}

{% block body %}
    <div class="page-heading title-lg">
        <div class="container">
            <h3>
                {{departements.NomDep}}
            </h3>
         </div>
        </div>
    <section class="subjects text-center" >
        <div class="container">
            <div class="big-title">

            </div>

            <div class="row fi">
                {% for filiere in departements.filieres %}
                <div class="col-xs-12 col-sm-6 col-md-4 col-lg-4 ">
                    <a href="{{ path('Filiere',{id:filiere.id,slug:filiere.slug})}}">
                    <div class="subject-box">

                            <div class="box-title">
                                  {{ filiere.nom }}

                            </div>

                     </div>
                    </a>
                </div>
                {% endfor %}
             </div>
        </div>
    </section>
{% endblock %}
